{
	"version": 3,
	"routes": [
		{
			"src": "/\\/tags",
			"headers": {
				"Location": "/tags/astro"
			},
			"status": 301
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/.*",
			"dest": "render"
		}
	]
}